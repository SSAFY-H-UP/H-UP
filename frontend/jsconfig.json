{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@api/*": ["@core/api/*"],
      "@hook/*": ["@core/hooks/*"],
      "@util/*": ["@core/utils/*"],
      "@asset/*": ["assets/*"],
      "@component/*": ["components/*"],
      "@constant/*": ["constants/*"],
      "@data/*": ["data/*"],
      "@page/*": ["pages/*"],
      "@recoil/*": ["recoil/*"]
    }
  },
  "include": ["src"]
}
